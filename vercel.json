{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/data",
      "dest": "/api/data.js"
    },
    {
      "src": "/api/tilok",
      "dest": "/api/tilok.js"
    },
    {
      "src": "/api/search",
      "dest": "/api/search.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
